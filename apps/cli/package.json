{
  "name": "@launchkey/cli",
  "version": "0.0.1",
  "description": "CLI for launchkey",
  "type": "module",
  "bin": {
    "better-webhook": "dist/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm run build",
    "prepare": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "launchkey"
  ],
  "license": "MIT",
  "author": "Endalk <endalk200>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endalk200/launchkey.git"
  },
  "bugs": {
    "url": "https://github.com/endalk200/launchkey/issues"
  },
  "homepage": "https://github.com/endalk200/launchkey#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "undici": "^7.16.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@better-webhook/typescript-config": "workspace:*",
    "@types/node": "^24.3.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
